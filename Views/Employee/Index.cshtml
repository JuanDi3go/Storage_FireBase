@model List<EmployeeModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="panel-body">
    <a class="btn btn-success" href="@Url.Action("Create","Mantenedor")">Create new</a>
    <hr />
    <table class="table-bordered table">
        <thead>
            <tr>
            <th>Name</th>
            <th>phone</th>
            <th>UrlProfile</th>
       
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.Name</td>
                    <td>@employee.PhoneNumber</td>
                    <td><img src="@employee.UrlPicture" width="100" height="100"/></td>
              
                </tr>
            }
        </tbody>
    </table>
</div>